{
  "name": "guigui",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "git://github.com/superguigui/guigui.git"
  },
  "description": "GUI tools in commonjs",
  "main": "./src/index.js",
  "scripts": {
    "start": "budo example/example.js:example/example-build.js --live --open",
    "build": "browserify example/example.js | uglifyjs -nm > example/example-build.js",
    "test": "mochify --cover",
    "docs": "jsdoc src/index.js -d ./docs -R README.md -t node_modules/minami"
  },
  "keywords": [
    "gui",
    "guigui",
    "ui",
    "interface",
    "kit",
    "slider",
    "knobs"
  ],
  "author": "Guillaume Gouessan <guillaume.gouessan@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "browserify": "*",
    "budo": "*",
    "chai": "*",
    "color": "*",
    "jsdoc": "^3.4.2",
    "minami": "^1.1.1",
    "mochify": "*",
    "uglify-js": "*",
    "watchify": "*"
  },
  "dependencies": {
    "component-emitter": "^1.2.0",
    "inline-style-prefixer": "^2.0.4",
    "object-assign": "^4.1.0",
    "raf-loop": "^1.1.3",
    "simple-color-picker": "0.0.6"
  }
}
